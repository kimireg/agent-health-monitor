#!/usr/bin/expect -f

# Zeabur Deploy Automation Script
set timeout 120

spawn zeabur deploy -i=false --create --name agent-health-monitor

expect "Select project"
send "4\r"

expect {
    "Select one service" {
        send "1\r"
        exp_continue
    }
    "Service deployed successfully" {
        puts "\n✅ Deployment successful!"
        exit 0
    }
    timeout {
        puts "\n❌ Timeout waiting for deployment"
        exit 1
    }
}

expect eof